<script type="text/javascript">
  $(window).load(function() {
    $('#hero-featureSwap').orbit({
     animation: 'horizontal-push',
     animationSpeed: 1000,
     advanceSpeed: 5500,
     pauseOnHover: true,
     startClockOnMouseOut: true,
     startClockOnMouseOutAfter: 1000,
     directionalNav: true,
     captions: false,
     bullets: true,
     afterSlideChange: function(){
      var visibleTile = $(this).data('analytics');
      urchinTracker(visibleTile);
     } 
    });
    if( !$.cookie('aws-x-main') ) {
      urchinTracker('/clicktrack/homepage/non-customer/');
      urchinTracker($('#hero-featureSwap').children().first().data('analytics'));
    } else {
      urchinTracker('/clicktrack/homepage/customer/');
      urchinTracker($('#hero-featureSwap').children().first().data('analytics'));
    }
  });
</script>

<script src="/js/jquery-ui-1.9.2.min.js" type="text/javascript"></script>
<script type="text/javascript">
$(document).ready(function() {
  $('.marc-container').on('click', '.marc-swap-link', function() {
    var $this = $(this);
    // Remove old boxes/colors
    $('.marc-orange').removeClass('marc-orange');
    $('.marc-content-box:visible').fadeOut('fast', function() {
      // Select the new box, change link color
      var boxID = $this.attr('id');
      $('#marc-'+boxID).fadeIn('fast');
      $this.addClass('marc-orange');
    });
  });

  // Activate jQuery UI's tabs on the homepage
  $('.tabs').tabs();

  // Activate Fancybox
  $('.fancybox').fancybox();
});
</script>


  

