var pe_mckmostshared = { 
	channels: {
		"dflt" : "https://subscription.people.com/storefront/subscribe-to-people/link/1017956.html"
	},

	dfpGet: ticmAdUnits[ticmAdUnits.length-1][1],
	channel: ticmAdUnits[ticmAdUnits.length-1][2],
	link: function() { return this.formatForDCLK(this.channels[this.channel] || this.channels["dflt"]) },

	formatForDCLK: function(link){

	  var tcm_dfpGet = this.dfpGet;
	  var fpa_adid = "", fpa_crid = "";

	  if(tcm_dfpGet == "%c"){}
	  else {
		link = tcm_dfpGet+link.replace("://","%3a%2f%2f");
		fpa_adid = ticmAdUnits[ticmAdUnits.length-1][3];
		fpa_crid = ticmAdUnits[ticmAdUnits.length-1][4];
		
		if (fpa_adid) {
		  link+="&fpa_adid=" + fpa_adid;
		}
		if (fpa_crid) {
		  link+="&fpa_crid=" + fpa_crid;
		}
		if( (link.indexOf("?")==-1) && (link.indexOf("&")>-1) ) {
		  link=link.replace("&","?");
		}
	  }
	  return link;
	},
	 getJSONP: function(url, cb_name, cb) {
	     var script = document.createElement("script");
        script.type = "text/javascript";
        script.src = url;
        document.body.appendChild(script);
        var existing_cb = window[cb_name] || function(){};
        window[cb_name] = function() {
            try { 
                cb.apply(this, arguments);
            } catch(err) {}
            existing_cb.apply(this, arguments);
        }
	
       
    },
	buildTout: function() {
	    
       var link = this.link();
		var jsonURL="//subscription-assets.timeinc.com/prod/assets/themes/magazines/default/template-resources/js/selfpublishing/PE/most-shared/most-shared-v2.js",title_attr, headline_html;
        this.getJSONP(jsonURL, "tcm_onMostSharedDataReady", (function() {
		
            title_attr = headline_html= window.tcm_mostShared.text;

			 document.getElementById('mostshared').innerHTML=headline_html;
			 document.getElementById('mostshared').title=title_attr;
		}));


		/////////////////////
		//TOUT CREATIVE, EDIT AS NEEDED
		////////////////////

       
		var toutHTML = '<div class="tout">';
		toutHTML += '<div style="position: absolute; top: 0px; right: 0px;"><img height="10" border="0" width="110" alt="FROM CELEBWATCH APP" src="http://subscription-assets.people.com/prod/assets/themes/magazines/SUBS/templates/velocity/site/pe-mckmostshared/resources/images/from_celebwatch_app.png"></div>';
