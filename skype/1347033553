<script type="text/javascript">

    /*
    * Fix iOS5 cache problem with forced reload
    */

    if((/iphone|ipod|ipad.*os [56]{1}/gi).test(navigator.appVersion)) {

        window.onpageshow = function(evt) {

            if (evt.persisted) {
                
                document.body.style.display = "none";
                location.reload();
            }
        };
    }

    var _t      = {},
        _obj    = null;
    _t.pageName = "static/home";
    _t.channel  = "home";
    _t.prop5    = _t.eVar5 = "en";
    SKYPE.ResponsiveState.init();
    _t.prop54   =  SKYPE.CurrentResponsiveState;
    _t.eVar71 = "";
    _t.prop36   = "";
    
    
    if (typeof s_el != 'undefined'){
	    if (typeof s_el.trackPageProperties != 'undefined') {
	    	_obj = $.extend(_obj, _t, s_el.trackPageProperties);
	    }
    }
    
    if (_obj != null) {
    	
    	s.trackPage(_obj);
    	
    } else {
    	
    	s.trackPage(_t);
    	
    }
    
    $(function() {
        $(document).on('click.sc', 'a[data-sc]:not(select)', function(e) {
            
            if (typeof s_el[$(this).attr('data-sc')] == 'undefined')
                return;
            
            var tracking_obj = s_el[$(this).attr('data-sc')].click;
            
            if (typeof tracking_obj === 'function'){
                tracking_obj = tracking_obj.call(); 
            }
            
            if (tracking_obj.asPage === true) {
                s.trackPage(tracking_obj);
            } else {
                s.trackAction(tracking_obj);
            }
        });
    });
</script>

	

