(function($){
        
        window.demoloaded = false;
        window.tasks = function() {            


            $("body.edition_us").each(function(){
                    if ( ! window.lvcvablog ) {
                            $("#blog_roll div.entry_content").find("span.lvcvapgblog").each(function(){
                                    window.lvcvablog = true;
                                    $(this).parents(".entry_container").find("h1,h2").first().css({"width":"400px"}).after("<iframe frameborder='0' scrolling='no' class='ad-blogpost' src='"+eol.page.dartRender({type:"adi",keywords:"lvcvapgblog",tile: "9", dimensions : "120x60"})+"' ></iframe>");
                                    $(this).remove();
                                });
                        }



                    if ( ! window.fashion_week_fall_2012 ) {                        
                        if ( eol.page.time() >= eol.page.gmt(new Date("02/09/2012")) && eol.page.time() <= eol.page.gmt(new Date("02/17/2012")) ) {
                            $("div.blog_entry").each(function(i){
                                    window.fashion_week_spring_2012 = true;
                                    if ( $(this).find("div.entry_footer a").map(function(){return ( $(this).text().toLowerCase() == "fashion week fall 2012" ) ? true : null;}).size() > 0 || 
