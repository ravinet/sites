                                    	$(this).find("h1,h2").first().css({"width":"400px"}).after("<a class='ad-blogpost' href='http:\/\/www.covergirl.com\/collections\/tru-collection' target='_blank'><img src='http:\/\/images.eonline.com\/static\/sponsor\/covergirl\/images\/2012_fall_fashion_sponsor_logo.gif' \/><\/a>");
                                    }
                                });
                        }
                    }
                    
                    if ( ! window.lvcva ) {                        
                        if ( eol.page.time() >= eol.page.gmt(new Date("05/18/2012")) && eol.page.time() <= eol.page.gmt(new Date("5/24/2012")) ) {
