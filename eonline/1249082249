        window.closeLB = function() { 
            $("div#LightBox").hide();
        }

        var scanInterval = setTimeout(tasks, 1000 );
        $(document).ready(tasks);
        $(window).load(function(){tasks();clearTimeout(scanInterval);});

        window.dartUpdateToggleSkinDance = function() {
            
            var scanner = setInterval( function() {
                    if ( $("#header_interior").offset().left + $("#header_interior").outerWidth() > 500 ) {
                            $("body").prepend('<ol class="skin-choices"><li><a id="choice-2">rustic</a></li><li><a id="choice-3">urban</a></li><li><a id="choice-1">pizazz</a></li></ol>');
        	            dartUpdateToggledSkin({skins : [ {skinUrl : "/static/skins/us/images/shermanwilliams/bkg.pizazz.jpg", color: "#cbc5e9"}, {skinUrl : "/static/skins/us/images/shermanwilliams/bkg.rustic.jpg", color: "#9ea474"}, {skinUrl : "/static/skins/us/images/shermanwilliams/bkg.urban.jpg", color: "#8daaae"} ]}); 
                            clearInterval(scanner);
                    }
                }, 1000 );
        }

        window.dartUpdateToggledSkin = function( args ) {
            		
        	function setSkin( id ) {
                        var selected = args.skins[id] || {};
                        
                        sTrackWidgetClick("skins:sherman-williams:choice:" + $("ol.skin-choices a#choice-id").text() );
            		$("body").css( {"background-image" : "url("+selected.skinUrl+")", "background-color" : selected.color, "background-repeat":"no-repeat","background-attachment" : "fixed", "background-position" : "center top"});
            		eol.cookie("skinchoicesw", id + 1);            		
            		$("ol.skin-choices li a").css({"opacity" : "1"}).removeClass("active").eq(id).addClass("active").css({"opacity" : ".3"});
                    }
/*        	        	            
                $("ol.skin-choices a").hover(function(){
                                $(this).css({"opacity" : "1"}).addClass("peek");
            		},function(e){
            			$(this).removeClass("peek");
            			if ( $(e.target).is(".active") ) {
            				$(this).css({"opacity" : ".3"});
            			}
                    });
                    */
                $("ol.skin-choices a").bind("click", function(e) {
                    var index = parseInt( $(e.target).attr("id").replace(/^\w+-/, "") || "-1", 10);

                    if ( parseInt( ( eol.cookie("skinchoice") || "-1" ) ) != index ) {              
                        e.stopPropagation();
                        e.preventDefault();
                        setSkin( index - 1 );
                        sTrackWidgetClick("skins:sherman-williams:change:" + $(this).text() );
                    } 
            });
            
            function positionOptions() {
            	var picker = $("#header_interior").offset().left + $("#header_interior").outerWidth() + 5;
            	$("ol.skin-choices").css({"left": picker + "px"});            	
            }
            setSkin( parseInt( eol.cookie("skinchoicesw") || "1") -1 );
            $(window).bind("resize", positionOptions );
            setTimeout( function(){positionOptions();}, 1000 );
        };

        $(window).load(function(){
            if($.browser.msie && (parseInt($.browser.version.charAt(0),10) < 8)){
            	$("li.google-plus").remove();
            }
        });
        
        

        
})(jQuery);