function addSiteCat(c){c=c.split(",");var a=(window.location.protocol.toLowerCase().indexOf("https")!=-1)?"https":"http";var b=(location.host.indexOf("-stage")!=-1||location.host.indexOf("-content")!=-1||location.host.indexOf("localhost")!=-1||location.host.indexOf("webst")!=-1||location.host.indexOf("-dev")!=-1)?"/us/assets/metrics/":a+"://www.oracleimg.com/us/assets/metrics/";for(i=0;i<c.length;i++){document.write("<script type='text/javascript' src='"+b+c[i]+".js'><\/script>");}}

